			- company_options.img.source[1].media = '(min-width: 320px)'
			- company_options.img.width = 336
			- company_options.img.height = 171
			- company_options.logo = {}
			- company_options.logo.modifier = 'megafon'
			- company_options.scope = 'Телекоммуникации'
			- company_options.jobCount = 'более 1000'
