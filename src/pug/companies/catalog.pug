.catalog
	.catalog__settings
		button.catalog__view-btn.catalog__view-btn--list.catalog__view-btn--active(
			aria-label="Показать в виде списка"
		)
			+svg("list")(class="catalog__view-icon" width="30" height="16")
		button.catalog__view-btn.catalog__view-btn--grid(
			aria-label="Показать в виде карточек"
		)
			+svg("cards")(class="catalog__view-icon" width="23" height="23")
		form.catalog__form(
			action="?"
			method="post"
		)
			label.catalog__sort
				select.catalog__sort-wrap(
					name="sorting"
				)
					option.catalog__sort(
						selected=true
						value="popularity"
					)
						| Популяность
					option.catalog__sort(
						value="posted"
					)
						| Дата
		p.catalog__total
			| Показано
			|
			span.catalog__total-min
				| 1
			| &#45;
			span.catalog__total-max
				| 8
			|
			| из
			|
			span.catalog__total-count
				| 32
			|
			| компаний
	ul.catalog__list
